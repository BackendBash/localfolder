public class min {
  public static void main(String[] args) {
	  int[] arr= {25,56,45,80,12};
	  System.out.println(min(arr));
	 
  }
  
  static int min(int arr[]) {
	  int ans=arr[0];
	  for(int i=1;i<arr.length;i++) {
		   if(ans>arr[i]) {
			   ans=arr[i];
			   
		   }
	  }
	  return ans;
  }
}
    